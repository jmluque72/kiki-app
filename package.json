{
  "name": "KikiApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "test:clear-cache": "vitest run --clearCache",
    "android:build:bundle": "cd android && ./gradlew bundleRelease",
    "android:build:apk": "cd android && ./gradlew assembleRelease",
    "android:clean": "cd android && ./gradlew clean",
    "ios:build:local": "ENVFILE=.env.local xcodebuild -workspace ios/KikiApp.xcworkspace -scheme KikiApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
    "ios:build:uat": "ENVFILE=.env.uat xcodebuild -workspace ios/KikiApp.xcworkspace -scheme KikiApp -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
    "ios:build:prod": "ENVFILE=.env.prod xcodebuild -workspace ios/KikiApp.xcworkspace -scheme KikiApp -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
    "ios:build:local:device": "ENVFILE=.env.local xcodebuild -workspace ios/KikiApp.xcworkspace -scheme KikiApp -configuration Release -sdk iphoneos -derivedDataPath ios/build CODE_SIGN_IDENTITY=\"\" CODE_SIGNING_REQUIRED=NO",
    "ios:build:uat:device": "ENVFILE=.env.uat xcodebuild -workspace ios/KikiApp.xcworkspace -scheme KikiApp -configuration Release -sdk iphoneos -derivedDataPath ios/build CODE_SIGN_IDENTITY=\"\" CODE_SIGNING_REQUIRED=NO",
    "ios:build:prod:device": "ENVFILE=.env.prod xcodebuild -workspace ios/KikiApp.xcworkspace -scheme KikiApp -configuration Release -sdk iphoneos -derivedDataPath ios/build CODE_SIGN_IDENTITY=\"\" CODE_SIGNING_REQUIRED=NO",
    "ios:run:local": "ENVFILE=.env.local npx react-native run-ios",
    "ios:run:uat": "ENVFILE=.env.uat npx react-native run-ios",
    "ios:run:prod": "ENVFILE=.env.prod npx react-native run-ios",
    "ios:archive:uat": "./scripts/ios-archive.sh uat",
    "ios:archive:prod": "./scripts/ios-archive.sh prod",
    "android:bundle:uat": "cd android && ENVFILE=../.env.uat ./gradlew clean bundleRelease",
    "android:bundle:prod": "cd android && ENVFILE=../.env.prod ./gradlew clean bundleRelease",
    "build:uat": "npm run ios:archive:uat && npm run android:bundle:uat",
    "build:prod": "npm run ios:archive:prod && npm run android:bundle:prod",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test": "detox test",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:test:auth": "detox test e2e/auth.e2e.js",
    "e2e:test:login": "detox test e2e/login-automated.e2e.js",
    "e2e:test:login:ios": "detox test --configuration ios.sim.debug e2e/login-automated.e2e.js",
    "e2e:test:login:android": "detox test --configuration android.emu.debug e2e/login-automated.e2e.js",
    "e2e:test:activities": "detox test e2e/activities.e2e.js",
    "e2e:test:attendance": "detox test e2e/attendance.e2e.js",
    "e2e:test:actions": "detox test e2e/student-actions.e2e.js",
    "e2e:test:forms": "detox test e2e/forms.e2e.js",
    "e2e:auto:ios": "bash scripts/run-e2e-automated.sh ios",
    "e2e:auto:android": "bash scripts/run-e2e-automated.sh android",
    "e2e:auto:all": "bash scripts/run-e2e-all.sh"
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.2.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.908.0",
    "@react-native-async-storage/async-storage": "^1.22.0",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-community/toolbar-android": "^0.2.1",
    "@react-native-picker/picker": "^2.11.1",
    "@react-native/new-app-screen": "0.80.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/drawer": "^7.5.7",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "aws-amplify": "^6.15.7",
    "axios": "^1.11.0",
    "expo-linear-gradient": "^14.1.5",
    "metro-cache": "^0.83.3",
    "react": "19.1.0",
    "react-native": "^0.80.0",
    "react-native-camera-kit": "16.0.1",
    "react-native-config": "^1.6.1",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-image-resizer": "^1.4.5",
    "react-native-permissions": "^5.4.2",
    "react-native-push-notification": "^8.1.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "^15.0.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-video": "^6.16.1",
    "react-native-webview": "^13.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "^20.0.1",
    "@react-native-community/cli-platform-android": "^19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.0",
    "@react-native/eslint-config": "0.80.0",
    "@react-native/metro-config": "0.80.0",
    "@react-native/typescript-config": "0.80.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.4.5",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "detox": "^20.17.0",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  }
}
